{
  "bot": {
    "name": "WhatsApp Bot",
    "prefix": "!",
    "version": "2.0.0"
  },
  "whatsapp": {
    "clientId": "whatsapp-bot-production",
    "headless": true
  },
  "dashBoard": {
    "enabled": true,
    "port": 10000
  },
  "autoRestart": {
    "enabled": true,
    "schedule": "0 2 * * *",
    "delay": 5000
  },
  "autoUptime": {
    "enabled": true,
    "interval": 3600000
  },
  "rateLimiting": {
    "maxRequests": 25,
    "windowMs": 60000,
    "minDelay": 1200,
    "maxDelay": 10000
  },
  "hideNotiMessage": {
    "commandNotFound": false
  },
  "adminBot": [
    "YOUR_PHONE_NUMBER@c.us"
  ]
}
